<Window x:Class="MVVM_Practice.MVVM_Command.View.MVVMCommand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVM_Practice.MVVM_Command.View"
        mc:Ignorable="d"
        Title="MVVMCommand" Height="450" Width="800">
    <Grid>
        <Button Command="{Binding ExecuteClick}"
            Margin="100" Content="Command Binding" FontSize="32"/>
    </Grid>
</Window>
